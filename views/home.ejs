    <%- include('partials/header.ejs')  %> 
    <!-- HEADER END HERE -->

    <!-- HERO AREA START HERE -->
    <section class="hero-area overly-bg" data-stellar-background-ratio=".9" style="background-image: url(/images/banner/hero.jpg)">
        <div class="container">

            <div class="row">
                <div class="col-md-12">
                    <div class="hero-wrap">
                        <h2 class="hero-title">785500+ Items For Sale</h2>
                        <p class="hero-intro">We Are specialize in creating some of the Best Free and Premium Bootstrap PSD Templates, HTML Templates, Wordpress Themes & Others Graphics</p>
                        <form action="#" class="item-search">
                            <input type="text" placeholder="Search your Items">
                        </form>
                        <div class="n-search-list">
                            <ul>
                                <% for(var i=0;i<=categories.length-1;i++) {%> 
                                <li data-filter=".cat1"><a href="/categories?category=<%= categories[i].category.split(' ').join('+') %>" ><%= categories[i].category %></a></li>
                                <% } %> 
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- HERO AREA END HERE -->

    <!-- TOP SELLING PRODUCT AREA START HERE -->
    <section class="feature-product-area section-padding">
        <div class="container">
 
            <div class="row">
                <div class="col-md-12">
                    <div class="section-intro">
                        <h2 class="section-title">Top Selling</h2>
                    </div>
                </div>
            </div>
            <div class="row">
              <div class="choose-wrap owl-carousel ">
                <!-- Single Product Start -->
            <% for(var i=products.length-1 ; i>=0  ;i--){ %> 
                <%if(products[i].topselling == true) {%> 
                <div class="col-md-3 col-sm-6">
                    <div class="item-demo" style="width: 300px">
                        <figure>
                            <div id="product<%= products[i].id %>" class="carousel slide" data-ride="carousel">
                                <!-- <ol class="carousel-indicators">
                                  <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                                  <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                  <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                                </ol> -->
                                <div class="carousel-inner">
                                <div class="carousel-item  active">
                                    <img class="d-block w-100" src="/images/uploads/<%= products[i].images[0] %>" alt="Images">
                                </div>
                                </div>
                                <a class="carousel-control-prev" href="#product<%= products[i].id  %>" role="button" data-slide="prev">
                                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                  <span class="sr-only">Previous</span>
                                </a>
                                <a class="carousel-control-next" href="#product<%= products[i].id %>" role="button" data-slide="next">
                                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                  <span class="sr-only">Next</span>
                                </a>
                            </div>
                            <div class="product-caption">
                                <div class="caption-cel">
                                    <div class="product-link">
                                        <div>
                                            <div>
                                                <a href="/products/details/<%= products[i].id %>">View Details <span><i class="fa fa-eye"></i></span></a>
                                            </div>
                                            <div>
                                                <a href="/cart/add/<%= products[i].id %>/?plan=basic">Add to Cart<span><i class="fa fa-shopping-cart"></i></span></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </figure>
                        <div class="product-info">
                            <div class="product-header">
                                <h3 class="product-name"><a href="/products/details/<%= products[i].id %>"><%= products[i].title %></a> 
                                    <% if(settings.currency == 'IND') {%> 
                                        <span> <i class="fa fa-rupee" style="color:#533396"></i> <%= products[i].basicPrice %> </span>
                                     <% }else{ %>
                                         <span><i class="fa fa-dollar" style="color:#533396"></i><%= products[i].basicPrice %> </span>
                                     <% } %>
                                     
                                </h3>
                                <!-- <span class="p-author">
                                    <a href="#">@admin</a>
                                </span> -->
                            </div>
                            <div class="product-meta">
                                <span class="meta-download">
                                    <i class="fa fa-cloud-download"></i><%= products[i].orders %> 
                                </span>
                                <span class="meta-love">
                                    <i class="fa fa-heart"></i><%= products[i].favorites %> 
                                </span>
                                <span class="meta-rating">
                                    <% for(var k=1;k<=5;k++){ %> 
                                        <% if(k<=products[i].rating){ %> 
                                        <i class="fa fa-star"></i>
                                        <% }else{ %>
                                        <i class="fa fa-star-o"></i>
                                        <% } %> 
                                    <% } %>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            <% } %> 
                <!-- Single Product End -->
            <% } %> 

            </div>
        </div>
    </section>
